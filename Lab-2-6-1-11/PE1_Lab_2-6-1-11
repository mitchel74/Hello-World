hour = int(input("Starting time (hours): "))
mins = int(input("Starting time (minutes): "))
dura = int(input("Event duration (minutes): "))

# Write your code here.
mins = mins + dura
#print(mins % 60)
#print(mins // 60)
hour = hour + mins // 60
#print(hour)

print(hour % 24, ":", mins % 60, sep="")